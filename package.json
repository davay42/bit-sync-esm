  },
  "keywords": [
    "rsync",
    "delta",
    "sync",
    "binary",
    "diff",
    "patch",
    "webtorrent",
    "browser",
    "esm",
    "webworker",
    "adler32",
    "rolling-checksum"
  ],
  "author": "Based on original by Clayton C. Gulick",
  "license": "MIT",
  "dependencies": {
    "@noble/hashes": "^1.5.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/bit-sync-esm.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bit-sync-esm/issues"
  },
  "homepage": "https://github.com/yourusername/bit-sync-esm#readme"
